<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_gragh"
    app:startDestination="@id/homeFrag">

    <fragment
        android:id="@+id/homeFrag"
        android:name="com.example.a33_navigation.HomeFragment"
        android:label="홈 프래그먼트"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFrag_to_secondFrag"
            app:destination="@id/secondFrag"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
    </fragment>
    <fragment
        android:id="@+id/secondFrag"
        android:name="com.example.a33_navigation.SecondFrag"
        android:label="세컨 프래그먼트"
        tools:layout="@layout/fragment_second" >
        <argument
            android:name="text"
            app:argType="string" />
    </fragment>
</navigation>